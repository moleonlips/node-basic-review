<!DOCTYPE html>
<html lang="en">

<head> <%- include('../partials/head.ejs') %> </head>

<body>

  <%- include('../partials/navbar.ejs'); %>

    <div class="container">
      <h3>
        <%= message %>
      </h3>
      <% if(newRoom.length !=0) { %>
        <p>Room info:</p>
        <p>
          - room: <%= newRoom.room %> <br>
            - length: <%= newRoom.length %> <br>
              - height: <%= newRoom.height %> <br>
                - width: <%= newRoom.width %> <br>
                  - area: <%= newRoom.length * newRoom.width %> <br>
                    - capacity: <%= newRoom.capacity %>
        </p>
        <% } %>
    </div>
</body>

</html>